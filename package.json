    "angular-playground": "^7.0.3",
    "asciidoctor.js": "^1.5.9",
    "codelyzer": "6.0.0",
    "conventional-changelog": "^3.1.21",
    "cypress": "^4.12.1",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "8.2.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "6.11.0",
    "front-matter": "^4.0.2",
