    "open": "^7.3.0",
    "osascript": "^1.2.0",
    "path-to-regexp": "^6.1.0",
    "prettier": "2.2.1",
    "pretty-quick": "^2.0.1",
    "protractor": "7.0.0",
    "start-server-and-test": "^1.11.3",
    "symlink-dir": "^4.1.0",
    "ts-jest": "26.4.0",
    "ts-node": "^9.1.1",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.1",
    "tslint": "6.1.3",
