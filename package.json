    "@types/minimatch": "^3.0.5",
    "@types/semver": "^7.3.8",
    "@types/ws": "^7.4.7",
    "asciidoctor": "^2.2.5",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "core-js": "^3.17.3",
    "cors": "^2.8.5",
    "critical": "^4.0.1",
    "critters": "^0.0.14",
    "cz-customizable": "^5.5.0",
    "express": "4.17.1",
    "got": "^11.8.2",
    "jest-preset-angular": "10.0.0",
    "level": "^7.0.0",
    "lint-staged": "^12.1.2",
    "minimatch": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "os": "^0.1.2",
    "playwright": "^1.17.1",
    "prismjs": "^1.25.0",
    "puppeteer": "^12.0.1",
    "puppeteer-core": "^12.0.1",
    "rxjs": "^7.3.0",
    "semver": "^7.3.5",
    "tslib": "^2.0.0",
    "ws": "^8.2.2",
    "yaml": "^2.0.0-9",
    "zone.js": "0.11.4"
  },
