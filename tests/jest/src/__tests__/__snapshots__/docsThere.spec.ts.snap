  route:
                  <span class="hljs-built_in"
                        =""
                  >
                    string
                  </span>
                  ;
  title?:
                  <span class="hljs-built_in"
                        =""
                  >
                    string
                  </span>
                  ;
  slugs?:
                  <span class="hljs-built_in"
                        =""
                  >
                    string
                  </span>
                  [];
  published?:
                  <span class="hljs-built_in"
                        =""
                  >
                    boolean
                  </span>
                  ;
  slug?:
                  <span class="hljs-built_in"
                        =""
                  >
                    string
                  </span>
                  ;
  [prop:
                  <span class="hljs-built_in"
                        =""
                  >
                    string
                  </span>
                  ]:
                  <span class="hljs-built_in"
                        =""
                  >
                    any
                  </span>
                  ;
}
                </code>
              </pre>
