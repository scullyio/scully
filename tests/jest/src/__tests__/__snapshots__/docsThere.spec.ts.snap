<h2 id=\\"scully-config-interface\\" =\\"\\">Scully Config Interface</h2>
<pre =\\"\\"><code class=\\"language-ts\\" =\\"\\"><span class=\\"hljs-keyword\\" =\\"\\">export</span> <span class=\\"hljs-keyword\\" =\\"\\">interface</span> ScullyConfig {
  <span class=\\"hljs-comment\\" =\\"\\">/** is this a bare project (without angular.json?) */</span>
  bareProject?: <span class=\\"hljs-built_in\\" =\\"\\">boolean</span>;
  <span class=\\"hljs-comment\\" =\\"\\">/** the name of the project we are using. Provided by Scully itself */</span>
  projectName?: <span class=\\"hljs-built_in\\" =\\"\\">string</span>;
