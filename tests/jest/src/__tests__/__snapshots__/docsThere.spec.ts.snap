                  </span>
                  hostName?:
                  <span class="hljs-built_in"
                        =""
                  >
                    string
                  </span>
                  ;
                  <span class="hljs-comment"
                        =""
                  >
                    /** optional hostURL, if this is provided, we are going to use this server instead of the build-in one. */
                  </span>
                  hostUrl?:
                  <span class="hljs-built_in"
                        =""
                  >
                    string
                  </span>
                  ;
                  <span class="hljs-comment"
                        =""
                  >
